{
  "name": "mobx-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.0",
    "mobx-react-devtools": "^4.2.11",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.11"
  },
  "craBabelExtend": {
    "presets": [
      "stage-0",
      "react-optimize"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "scripts": {
    "start": "cra-babel-extend && react-scripts start",
    "build": "cra-babel-extend && react-scripts build",
    "test": "cra-babel-extend && react-scripts test --env=jsdom",
    "eject": "cra-babel-extend && react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "cra-babel-extend": "^1.0.3"
  }
}
