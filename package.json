{
  "name": "cra-babel-extend",
  "version": "1.0.1",
  "description": "Inject Babel plugins and presets into Create React App's react-scritps",
  "main": "dist/index.js",
  "author": "Evan M Jones <evanjones4040@gmail.com> (http://evanjon.es/)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "test": "node test",
    "postinstall": "babel src --out-dir dist --plugins=transform-es2015-modules-commonjs --presets=es2015",
    "clean": "rm -rf node_modules dist"
  },
  "bin": {
    "cra-babel-extend": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mini-eggs/cra-babel-extend.git"
  }
}
